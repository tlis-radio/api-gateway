{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "route1" : {
        "ClusterId": "cms-usermanagement",
        "Match": {
          "Path": "/usermanagement/{**catch-all}"
        },
        "Transforms": [{ "PathPattern": "{**catch-all}" }]
      }
    },
    "Clusters": {
      "cms-usermanagement": {
        "Destinations": {
          "Default": {
            "Address": "https://localhost:7152/",
            "Swaggers": [
              {
                "PrefixPath": "/usermanagement",
                "Paths": [
                  "swagger/v1/swagger.json"
                ]
              }
            ]
          }
        }
      }
    }
  }
}
